{
  "name": "Govee BT RGB Strip",
  "description": "Control Govee Bluetooth RGB Light Strips via SignalRGB",
  "version": "1.0.0",
  "main": "start.js",
  "type": "plugin",
  "config": {
    "fields": [
      {
        "id": "api_key",
        "type": "text",
        "label": "Govee API Key",
        "default": "",
        "required": true
      },
      {
        "id": "device_id",
        "type": "text",
        "label": "Device ID",
        "default": "",
        "required": true
      },
      {
        "id": "device_model",
        "type": "text",
        "label": "Device Model",
        "default": "",
        "required": true
      }
    ]
  }
}
